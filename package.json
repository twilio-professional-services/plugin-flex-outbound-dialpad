{
  "name": "plugin-outbound-dialing-with-conference",
  "description": "Example front end implementation of a dialpad that creates outbound calls via a voice sdk and sends call to worker via task router",
  "version": "1.3.1",
  "author": "Jared Hunter <jhunter@twilio.com>",
  "private": true,
  "scripts": {
    "bootstrap": "flex-plugin check-start",
    "prebuild": "rimraf build && npm run bootstrap",
    "build": "./node_modules/.bin/env-cmd -f serverless/.env flex-plugin build",
    "clear": "flex-plugin clear",
    "predeploy": "npm run build",
    "deploy": "flex-plugin deploy",
    "eject": "flex-plugin eject",
    "info": "flex-plugin info",
    "postinstall": "npm run bootstrap",
    "list": "flex-plugin list",
    "remove": "flex-plugin remove",
    "prestart": "npm run bootstrap",
    "start": "./node_modules/.bin/env-cmd -f serverless/.env flex-plugin start",
    "test": "flex-plugin test --env=jsdom"
  },
  "dependencies": {
    "craco-config-flex-plugin": "^3.10.1",
    "flex-plugin": "^3.5.0",
    "flex-plugin-scripts": "^3.11.0",
    "react": "16.5.2",
    "react-click-n-hold": "^1.0.7",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.6",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@twilio/flex-ui": "^1.19.0",
    "babel-polyfill": "^6.26.0",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.0"
  },
  "config-overrides-path": "node_modules/react-app-rewire-flex-plugin",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
